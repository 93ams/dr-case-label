{
  "name": "dr-case-label",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "main": "dist/server/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:next && npm run build:nest",
    "build:next": "next build",
    "build:nest": "nest build --path ./tsconfig.server.json",
    "build:docker": "docker build -f ./Dockerfile -t dr-case-label .",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "cypress run",
    "test:open": "cypress open",
    "start": "cross-env NODE_ENV=production node dist/server/main.js",
    "start:next": "next dev",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch --path tsconfig.server.json",
    "start:debug": "nest start --path ./tsconfig.server.json --debug --watch",
    "start:docker": "docker run --rm -p 3000:3000 -p 7655:22 -it $(docker build --build-arg ssh_pub_key=\"$(cat ~/.ssh/id_rsa.pub)\" -q .)",
    "start:ssh": "ssh -o StrictHostKeyChecking=no -L 9221:localhost:9229 root@localhost -p 7655",
    "start:prod": "node dist/main",
    "start:proxy": "docker run --name nest-next-blog-proxy -v $(pwd)/nginx.conf:/etc/nginx/conf.d/default.conf:ro --add-host localnode:$(ifconfig en0 | grep inet | grep -v inet6 | awk '{print $2}') -p 8080:8080 -d nginx",
    "lint": "eslint \"{src,apps,libs,test}/**/*.tsx?\" --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.6.2",
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@nestjs/apollo": "^10.0.11",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.11",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "apollo-server-express": "^3.7.0",
    "bcrypt": "^5.0.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.5.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.3",
    "nest-commander": "^2.5.0",
    "nest-next": "^10.0.0",
    "next": "^12.1.6",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-window": "^1.8.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@casl/ability": "^5.4.3",
    "@cypress/webpack-preprocessor": "^5.11.1",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@types/react-window": "^1.8.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cypress": "^9.6.1",
    "cypress-dark": "^1.1.0",
    "cypress-mongodb": "^3.0.1",
    "eslint": "^8.0.1",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "neat-csv": "^7.0.0",
    "prettier": "^2.3.2",
    "run-script-webpack-plugin": "^0.0.11",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
